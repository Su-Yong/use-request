{
  "name": "@suyongs/use-request",
  "version": "0.5.0",
  "description": "Data fetch library for React",
  "main": "dist/use-request.umd.js",
  "module": "dist/use-request.esm.js",
  "types": "dist/use-request.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "npm run clean && npm run build:only",
    "build:only": "rollup -c",
    "build:docs": "cd ./website && npm run build",
    "prepublish": "npm run test && npm run build",
    "example:dev": "cd ./example && npm run dev",
    "release": "release-it",
    "docs": "cd ./website && npm run start"
  },
  "keywords": [
    "react",
    "ajax",
    "request"
  ],
  "author": "Su-Yong",
  "license": "MIT",
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^8.0.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.5",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "react": "^17.0.2",
    "release-it": "^15.1.0",
    "rollup": "^2.70.0",
    "rollup-plugin-dts": "^4.2.0",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  }
}
